{
  "name": "qwik-media-recorder",
  "version": "1.0.1",
  "description": "Qwik hook for record audio from microphone",
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chuikoffru/qwik-media-recorder.git"
  },
  "author": {
    "name": "Konstantin Chuykov",
    "email": "chuikoff.ru@gmail.com"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs",
      "types": "./lib-types/index.d.ts"
    }
  },
  "files": [
    "lib",
    "lib-types"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "private": false,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "release": "np",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.2.18",
    "@types/eslint": "8.44.7",
    "@types/node": "^20.9.2",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "eslint": "8.54.0",
    "eslint-plugin-qwik": "latest",
    "np": "8.0.4",
    "prettier": "3.1.0",
    "typescript": "5.2.2",
    "undici": "5.27.2",
    "vite": "5.0.0",
    "vite-tsconfig-paths": "4.2.1"
  }
}
